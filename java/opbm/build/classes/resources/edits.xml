				<up>listbox_move_up</up>
				<down>listbox_move_down</down>
				<source>opbm.scriptdata.abstracts.abstract</source>
				<foreach>abstract</foreach>
				<listby p1="#name"/>
			</listbox>
			<label zoom="main" text="Abstraction Name / Description:"/>
			<textbox zoom="main" field="#name"/>
			<label zoom="main" text="for Scripting Engine:"/>
			<textbox zoom="main" default="autoit" field="engine"/>
			<vspacer zoom="main" height="10"/>
			<label zoom="main" text="OPBM Command:"/>
			<textbox zoom="main" width="350" field="logical"/>
			<vspacer zoom="main" height="10"/>
			<label zoom="main" text="Scripting Engine Command:"/>
			<editbox height="400" zoom="main" field="real"/>
		</edit>

		<edit name="atoms">
			<precommand p1="Edit Atoms">leftpanel</precommand>
			<label width="250" text="Atom Name/Description:"/>
			<textbox width="250" field="#name"/>
			<label width="250" text="Atoms Listing:"/>
			<listbox name="atom">
				<add>listbox_add</add>
				<delete>listbox_delete</delete>
				<clone>listbox_clone</clone>
				<onselect p1="elements">lookupbox_update</onselect>
				<source>opbm.scriptdata.atoms.atom</source>
				<foreach>atom</foreach>
				<listby p1="#name"/>
			</listbox>
			<label width="250" text="Operation Name/Description:"/>
			<textbox width="250" field="elements.#name"/>
			<label width="250" text="Sequence of Operations:"/>
			<lookupbox name="elements" autoupdate="no" height="max">
				<subtract>lookupbox_subtract</subtract>
				<zoom p1="relativeto" autopreview="yes">lookupbox_zoom</zoom>
				<up>lookupbox_move_up</up>
				<down>lookupbox_move_down</down>
				<source relativeto="atom">[flow,custom,abstract]</source>
				<foreach>[flow,custom,abstract]</foreach>
				<relativeedits p1="flow=flows" p2="custom=customs" p3="abstract=abstracts"/>
				<relativeoptions p1="flow=opbm.scriptdata.flows.flow" p2="custom=opbm.scriptdata.customs.custom" p3="abstract=opbm.scriptdata.abstracts.abstract"/>
				<listby p1="#name"/>
			</lookupbox>
			<newcolumn/>
			<label text="Flow Control:"/>
			<lookupbox name="flows" autoupdate="no">
				<add p1="atom" p2="elements" p3="flows">lookupbox_add</add>
				<zoom p1="flows" autopreview="yes">lookupbox_zoom</zoom>
				<source>opbm.scriptdata.flows.flow</source>
				<foreach>flow</foreach>
				<listby p1="#name"/>
			</lookupbox>
			<label text="Custom Inserts:"/>
			<lookupbox name="customs" autoupdate="no">
				<add p1="atom" p2="elements" p3="customs">lookupbox_add</add>
				<zoom p1="customs" autopreview="yes">lookupbox_zoom</zoom>
				<source>opbm.scriptdata.customs.custom</source>
				<foreach>custom</foreach>
				<listby p1="#name"/>
			</lookupbox>
			<label text="Command Abstracts:"/>
			<lookupbox name="abstracts" autoupdate="no">
				<add p1="atom" p2="elements" p3="abstracts">lookupbox_add</add>
				<zoom p1="abstracts" autopreview="yes">lookupbox_zoom</zoom>
				<source>opbm.scriptdata.abstracts.abstract</source>
				<foreach>abstract</foreach>
				<listby p1="#name"/>
			</lookupbox>
			<newcolumn/>
			<options relativeto="elements"/>
		</edit>

