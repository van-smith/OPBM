<?xml version="1.0" encoding="utf-8"?>
<opbm>
<!-- rawedits define full file displays, simply loaded in and viewed or edited, like a text editor. -->
	<rawedits>
		<comment>All rawedit items are read-write by default.</comment>
		<rawedit name="edits.xml">
			<precommand p1="Raw Edit">leftpanel</precommand>
			<filename>$edits.xml$</filename>
			<tooltip>Editing: $edits.xml$</tooltip>
			<postcommand></postcommand>
		</rawedit>
		<rawedit name="panels.xml">
			<precommand p1="Raw Edit">leftpanel</precommand>
			<filename>$panels.xml$</filename>
			<tooltip>Editing: $panels.xml$</tooltip>
		</rawedit>
		<rawedit name="scripts.xml">
			<precommand p1="Raw Edit">leftpanel</precommand>
			<filename>$scripts.xml$</filename>
			<tooltip>Editing: $scripts.xml$</tooltip>
		</rawedit>
		<comment>The following are viewing items, which are read-only raw edits.</comment>
		<rawedit name="ro:edits.xml" readonly="true">
			<precommand p1="Raw View">leftpanel</precommand>
			<filename>$edits.xml$</filename>
			<tooltip>Viewing: $edits.xml$</tooltip>
		</rawedit>
		<rawedit name="ro:panels.xml" readonly="true">
			<precommand p1="Raw View">leftpanel</precommand>
			<filename>$panels.xml$</filename>
			<tooltip>Viewing: $panels.xml$</tooltip>
		</rawedit>
		<rawedit name="ro:scripts.xml" readonly="true">
			<precommand p1="Raw View">leftpanel</precommand>
			<filename>$scripts.xml$</filename>
			<tooltip>Viewing: $scripts.xml$</tooltip>
		</rawedit>
	</rawedits>

	<edits>

		<edit name="flows">
			<precommand p1="Edit Flows">leftpanel</precommand>
			<listbox zoom="main">
				<add>listbox_add</add>
				<delete>listbox_delete</delete>
				<clone>listbox_clone</clone>
				<up>listbox_move_up</up>
				<down>listbox_move_down</down>
				<source>opbm.scriptdata.flows.flow</source>
				<foreach>flow</foreach>
				<listby p1="#name"/>
				<newRecordPattern>
					<flow name="New Flow">
						<variables>(leftbrace)(colon)example, Description of variable input field(colon)(rightbrace)</variables>
					</flow>
				</newRecordPattern>
			</listbox>
			<label zoom="main" text="Flow Control Directive Name:"/>
			<textbox readonly="yes" zoom="main" width="200" field="#name"/>
			<vspacer zoom="main" height="10"/>
			<label zoom="main" text="Variable Prompts:"/>
			<editbox readonly="yes" zoom="main" field="variables"/>
		</edit>

		<edit name="abstracts">
			<precommand p1="Edit Abstracts">leftpanel</precommand>
			<listbox zoom="main">
				<add>listbox_add</add>
				<delete>listbox_delete</delete>
				<clone>listbox_clone</clone>
				<up>listbox_move_up</up>
				<down>listbox_move_down</down>
				<source>opbm.scriptdata.abstracts.abstract</source>
				<foreach>abstract</foreach>
				<listby p1="#name"/>
				<newRecordPattern>
					<abstract name="New Abstract">
						<logical>name</logical>
						<real>(leftbrace)(colon)name, Description of the name entry(colon)(rightbrace)</real>
						<variables>(leftbrace)(colon)field1, Description of variable input field(colon)(rightbrace) (leftbrace)(colon)retryAttempts, Retry how many times? (Overrides settings.xml)(colon)(rightbrace) (leftbrace)(colon)stopIfFailure, Should this atom stop on a failure? (Overrides settings.xml)(colon)(rightbrace)</variables>
					</abstract>
				</newRecordPattern>
			</listbox>
			<label zoom="main" text="Abstraction Name / Description:"/>
			<textbox readonly="yes" zoom="main" field="#name"/>
			<vspacer zoom="main" height="10"/>
			<label zoom="main" text="OPBM Command:"/>
			<textbox readonly="yes" zoom="main" width="350" field="logical"/>
			<vspacer zoom="main" height="10"/>
			<label zoom="main" text="OPBM Variable Fields:"/>
			<editbox readonly="yes" zoom="main" height="max" field="real"/>
		</edit>

		<edit name="atoms">
			<precommand p1="Edit Atoms">leftpanel</precommand>
			<label width="250" text="Atom Name/Description:"/>
			<textbox width="250" field="#name"/>
			<label width="250" text="Atom Listing:"/>
			<listbox name="atom">
				<delete>listbox_delete</delete>
				<clone>listbox_clone</clone>
				<up>listbox_move_up</up>
				<down>listbox_move_down</down>
				<onselect p1="elements">lookupbox_update</onselect>
				<source>opbm.scriptdata.atoms.atom</source>
				<template>opbm.scriptdata.templates.atom</template>
				<foreach>atom</foreach>
				<listby p1="#name"/>
			</listbox>
			<label width="250" text="Operation Name/Description:"/>
			<textbox width="250" field="elements.#name"/>
			<label width="250" text="Sequence of Operations:"/>
			<lookupbox name="elements" autoupdate="no" height="max">
				<subtract>lookupbox_subtract</subtract>
				<clone>lookupbox_clone</clone>
				<up>lookupbox_move_up</up>
				<down>lookupbox_move_down</down>
				<zoom p1="relativeto" autopreview="yes">lookupbox_zoom</zoom>
				<source relativeto="atom">[flow,abstract]</source>
				<foreach>[flow,abstract]</foreach>
				<relativeedits p1="flow=flows" p2="abstract=abstracts"/>
				<relativeoptions p1="flow=opbm.scriptdata.flows.flow" p2="abstract=opbm.scriptdata.abstracts.abstract"/>
				<listby p1="#name"/>
			</lookupbox>
			<newcolumn/>
			<label text="Flow Control:"/>
			<lookupbox name="flows" autoupdate="no">
				<add p1="atom" p2="elements" p3="flows">lookupbox_add</add>
				<zoom p1="flows" autopreview="yes">lookupbox_zoom</zoom>
				<source>opbm.scriptdata.flows.flow</source>
				<foreach>flow</foreach>
				<listby p1="#name"/>
			</lookupbox>
			<label text="Command Abstracts:"/>
			<lookupbox name="abstracts" autoupdate="no">
				<add p1="atom" p2="elements" p3="abstracts">lookupbox_add</add>
				<zoom p1="abstracts" autopreview="yes">lookupbox_zoom</zoom>
				<source>opbm.scriptdata.abstracts.abstract</source>
				<foreach>abstract</foreach>
				<listby p1="#name"/>
			</lookupbox>
			<newcolumn/>
			<options relativeto="elements"/>
		</edit>

