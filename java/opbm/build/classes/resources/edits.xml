<?xml version="1.0" encoding="utf-8"?>
<opbm>
<!-- rawedits define full file displays, simply loaded in and viewed or edited, like a text editor. -->
	<rawedits>
		<comment>All rawedit items are read-write by default.</comment>
		<rawedit name="edits.xml">
			<precommand p1="Raw Edit">leftpanel</precommand>
			<filename>$edits.xml$</filename>
			<tooltip>Editing: $edits.xml$</tooltip>
			<postcommand></postcommand>
		</rawedit>
		<rawedit name="panels.xml">
			<precommand p1="Raw Edit">leftpanel</precommand>
			<filename>$panels.xml$</filename>
			<tooltip>Editing: $panels.xml$</tooltip>
		</rawedit>
		<rawedit name="scripts.xml">
			<precommand p1="Raw Edit">leftpanel</precommand>
			<filename>$scripts.xml$</filename>
			<tooltip>Editing: $scripts.xml$</tooltip>
		</rawedit>
		<comment>The following are viewing items, which are read-only raw edits.</comment>
		<rawedit name="ro:edits.xml" readonly="true">
			<precommand p1="Raw View">leftpanel</precommand>
			<filename>$edits.xml$</filename>
			<tooltip>Viewing: $edits.xml$</tooltip>
		</rawedit>
		<rawedit name="ro:panels.xml" readonly="true">
			<precommand p1="Raw View">leftpanel</precommand>
			<filename>$panels.xml$</filename>
			<tooltip>Viewing: $panels.xml$</tooltip>
		</rawedit>
		<rawedit name="ro:scripts.xml" readonly="true">
			<precommand p1="Raw View">leftpanel</precommand>
			<filename>$scripts.xml$</filename>
			<tooltip>Viewing: $scripts.xml$</tooltip>
		</rawedit>
	</rawedits>

<!-- edits define what data is displayed or input for on-screen GUI "forms" -->
	<edits>
		<edit name="flows">
			<precommand p1="Edit Flows">leftpanel</precommand>
			<listbox zoom="main">
				<add>listbox_add</add>
				<delete>listbox_delete</delete>
				<clone>listbox_clone</clone>
				<up>listbox_move_up</up>
				<down>listbox_move_down</down>
				<source>opbm.scriptdata.flows.flow</source>
				<foreach>flow</foreach>
				<listby p1="#name"/>
			</listbox>
			<label zoom="main" text="Flow Control Directive Name:"/>
			<textbox zoom="main" width="200" field="#name"/>
			<vspacer zoom="main" height="10"/>
			<label zoom="main" text="Variable Prompts:"/>
			<editbox zoom="main" field="variables"/>
		</edit>

		<edit name="customs">
			<precommand p1="Edit Customs">leftpanel</precommand>
			<listbox zoom="main">
				<add>listbox_add</add>
				<delete>listbox_delete</delete>
				<clone>listbox_clone</clone>
				<up>listbox_move_up</up>
				<down>listbox_move_down</down>
				<source>opbm.scriptdata.customs.custom</source>
				<foreach>custom</foreach>
				<listby p1="#name"/>
			</listbox>
			<label zoom="main" text="Custom Name:"/>
			<textbox zoom="main" field="#name"/>
			<label zoom="main" text="Tags:"/>
			<textbox zoom="main" field="tags"/>
			<vspacer zoom="main" height="10"/>
			<label zoom="main" text="Custom Insert:"/>
			<editbox zoom="main" height="max" field="insert"/>
		</edit>

