				<add p1="scenario" p2="elements" p3="molecules">lookupbox_add</add>
				<source>opbm.scriptdata.molecules.molecule</source>
				<foreach>molecule</foreach>
				<listby p1="#name"/>
			</lookupbox>
			<newcolumn/>
			<options relativeto="elements"/>
		</edit>

