<?xml version="1.0" encoding="utf-8"?>
<opbm>
	<scriptdata>
		<comment>Flows are flow control operations usable in scripting atoms, molecules, scenarios or suites</comment>
		<comment>Abstracts are definitions between our internal set of commands, and the underlying harness (and ultimately a scripting engine)</comment>
		<comment>Atoms are the smallest autonomous item, do a single thing</comment>
		<comment>Molecules are smallest group of autonomous item, does a sequence of things</comment>
		<comment>Scenarios are smallest group of sequences, does multiple sequences of things</comment>
		<comment>Suites are largest group of multiple sequences, does an entire range of things</comment>
		<flows>
			<flow name="call">
				<variables>(leftbrace)(colon)function, FUNCTION(colon)(rightbrace)</variables>
			</flow>
			<flow name="function">
				<variables>(leftbrace)(colon)function, FUNCTION name(colon)(rightbrace)</variables>
			</flow>
			<flow name="functionEnd">
				<variables></variables>
			</flow>
			<flow name="return">
				<variables></variables>
			</flow>
			<flow name="goto">
				<variables>(leftbrace)(colon)label, LABEL(colon)(rightbrace)</variables>
			</flow>
			<flow name="label">
				<variables>(leftbrace)(colon)label, LABEL name(colon)(rightbrace)</variables>
			</flow>
			<flow name="switch">
				<variables>(leftbrace)(colon)variable, VARIABLE(colon)(rightbrace)</variables>
			</flow>
			<flow name="case">
				<variables>(leftbrace)(colon)value, VALUE or VARIABLE(colon)(rightbrace)</variables>
			</flow>
			<flow name="break">
				<variables></variables>
			</flow>
			<flow name="default">
				<variables></variables>
			</flow>
			<flow name="switchEnd">
				<variables></variables>
			</flow>
			<flow name="do">
				<variables></variables>
			</flow>
			<flow name="until">
				<variables>(leftbrace)(colon)value1, Left(hyphen)side VALUE(colon)(rightbrace) (leftbrace)(colon)comparator, Logic(colon)(rightbrace) (leftbrace)(colon)value2, Right(hyphen)side VALUE(colon)(rightbrace)</variables>
			</flow>
			<flow name="while">
				<variables>(leftbrace)(colon)value1, Left(hyphen)side VALUE(colon)(rightbrace) (leftbrace)(colon)comparator, Logic(colon)(rightbrace) (leftbrace)(colon)value2, Right(hyphen)side VALUE(colon)(rightbrace)</variables>
			</flow>
			<flow name="whileEnd">
				<variables></variables>
			</flow>
			<flow name="for">
				<variables>(leftbrace)(colon)variable, Variable name(colon)(rightbrace) (leftbrace)(colon)startvalue, START or variable name(colon)(rightbrace) (leftbrace)(colon)finishvalue, END or variable name(colon)(rightbrace) (leftbrace)(colon)stepvalue, STEP or variable name(colon)(rightbrace)</variables>
			</flow>
			<flow name="next">
				<variables></variables>
			</flow>
			<flow name="if">
				<variables>(leftbrace)(colon)value1, Left(hyphen)side VALUE(colon)(rightbrace) (leftbrace)(colon)comparator, Logic(colon)(rightbrace) (leftbrace)(colon)value2, Right(hyphen)side VALUE(colon)(rightbrace)</variables>
			</flow>
			<flow name="elseif">
				<variables>(leftbrace)(colon)value1, Left(hyphen)side VALUE(colon)(rightbrace) (leftbrace)(colon)comparator, Logic(colon)(rightbrace) (leftbrace)(colon)value2, Right(hyphen)side VALUE(colon)(rightbrace)</variables>
			</flow>
			<flow name="else">
				<variables></variables>
			</flow>
			<flow name="ifEnd">
				<variables></variables>
			</flow>
			<flow name="log">
				<variables>(leftbrace)(colon)message, MESSAGE to log(colon)(rightbrace)</variables>
			</flow>
			<flow name="throw">
				<variables>(leftbrace)(colon)message, ERROR MESSAGE to throw(colon)(rightbrace)</variables>
			</flow>
			<flow name="debug">
				<variables>(leftbrace)(colon)message, DEBUG MESSAGE to show(colon)(rightbrace)</variables>
			</flow>
			<flow name="quit">
				<variables>(leftbrace)(colon)value, EXIT value(colon)(rightbrace)</variables>
			</flow>
			<flow name="prompt">
				<variables>(leftbrace)(colon)message, MESSAGE to prompt user with(colon)(rightbrace)</variables>
			</flow>
			<flow name="optionDuo">
				<variables>(leftbrace)(colon)message, MESSAGE to prompt user with(colon)(rightbrace) (leftbrace)(colon)option1, OPTION (poundsign)1 Prompt(colon)(rightbrace) (leftbrace)(colon)option2, OPTION (poundsign)2 Prompt(colon)(rightbrace)</variables>
			</flow>
			<flow name="onOption1">
				<variables>(leftbrace)(colon)label, Label if option (poundsign)1 chosen(colon)(rightbrace)</variables>
			</flow>
			<flow name="onOption2">
				<variables>(leftbrace)(colon)label, Label if option (poundsign)2 chosen(colon)(rightbrace)</variables>
			</flow>
			<flow name="variable">
				<variables>(leftbrace)(colon)name, NAME(colon)(rightbrace) (leftbrace)(colon)value, VALUE(colon)(rightbrace)</variables>
			</flow>
			<flow name="(plus)(plus)">
				<variables>(leftbrace)(colon)name, NAME(colon)(rightbrace)</variables>
			</flow>
			<flow name="(hyphen)(hyphen)">
				<variables>(leftbrace)(colon)name, NAME(colon)(rightbrace)</variables>
			</flow>
			<flow name="(plus)(equals)">
				<variables>(leftbrace)(colon)name, NAME(colon)(rightbrace) (leftbrace)(colon)value, VALUE(colon)(rightbrace)</variables>
			</flow>
			<flow name="(hyphen)(equals)">
				<variables>(leftbrace)(colon)name, NAME(colon)(rightbrace) (leftbrace)(colon)value, VALUE(colon)(rightbrace)</variables>
			</flow>
		</flows>
		<abstracts>
			<abstract name="execute">
				<logical>execute</logical>
				<real>(leftbrace)(colon)executable, Full executable path(colon)(rightbrace)</real>
			</abstract>
			<abstract name="executeParams">
				<logical>execute_params</logical>
				<real>(leftbrace)(colon)executable, Full executable path(colon)(rightbrace) (leftbrace)(colon)p1, First parameter(colon)(rightbrace) (leftbrace)(colon)p2, Second parameter(colon)(rightbrace) (leftbrace)(colon)p3, Third parameter(colon)(rightbrace) (leftbrace)(colon)p4, Fourth parameter(colon)(rightbrace) (leftbrace)(colon)p5, Fifth parameter(colon)(rightbrace) (leftbrace)(colon)p6, Sixth parameter(colon)(rightbrace) (leftbrace)(colon)p7, Seventh parameter(colon)(rightbrace) (leftbrace)(colon)p8, Eighth parameter(colon)(rightbrace) (leftbrace)(colon)p9, Ninth parameter(colon)(rightbrace) (leftbrace)(colon)p10, Tenth parameter(colon)(rightbrace)</real>
			</abstract>
			<abstract name="rebootAndTerminate">
				<logical>reboot_and_terminate</logical>
				<real></real>
			</abstract>
			<abstract name="rebootAndContinue">
				<logical>reboot_and_continue</logical>
				<real></real>
			</abstract>
			<abstract name="rebootAndRestart">
				<logical>reboot_and_restart</logical>
				<real></real>
			</abstract>
		</abstracts>
